alias zshconfig="subl ~/.zshrc"
alias ohmyzsh="subl ~/.oh-my-zsh ~/.zshrc"
alias homegit="git --work-tree=$HOME --git-dir=$HOME/.files.git"
alias wine64="WINEPREFIX='~/.local/share/wineprefixes/wine64' WINEARCH=64 wine"
alias wine="WINEPREFIX='~/.local/share/wineprefixes/wine32' WINEARCH=32 wine"
alias mail="mutt"
